    "babel-loader": "~6.2.10",
    "babel-preset-es2015": "~6.18.0",
    "bulma": "^0.2.3",
    "concurrently": "~3.1.0",
    "css-loader": "~0.26.1",
    "d3": "~4.4.0",
    "jspm": "~0.16.48",
    "live-server": "^1.1.0",
    "node-sass": "~4.1.1",
    "nodemon": "^1.11.0",
    "pouchdb": "~6.1.0",
    "pushstate-anchor": "~0.4.4",
    "sass-loader": "~4.1.1",
    "style-loader": "~0.13.1",
    "webcomponents.js": "~0.7.23",
    "webpack": "~2.2.0-rc.3"
  }
